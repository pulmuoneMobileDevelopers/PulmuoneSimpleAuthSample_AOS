<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- View가 눌렸을 때 @color/colorAccent 색상으로 출력한다.-->
    <item android:color="@color/colorSelectorPressed" android:state_pressed="true"/>

    <!-- View가 눌리지 않았을 때 @color/colorPrimaryDark 색상으로 출력한다.-->
    <item android:color="@color/colorSelectorDefault" android:state_pressed="false"/>
</selector>